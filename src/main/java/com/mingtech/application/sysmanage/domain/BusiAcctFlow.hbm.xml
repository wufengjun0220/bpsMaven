<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="BT_BUSI_ACCT_FLOW" name="com.mingtech.application.sysmanage.domain.BusiAcctFlow" dynamic-insert="true" dynamic-update="true">
    <id type="string" column="id" length="40" name="id">
      <generator class="uuid"/>
    </id>
    
   <property name="billId" length="60" type="string" column="BILL_ID"/>
   <property name="billInfoId" length="60" type="string" column="BILL_INFO_ID"/>
   <property name="prodId" length="40" type="string" column="PROD_ID"/>
   <property name="batchId" length="40" type="string" column="BATCH_ID"/>
   <property name="brachId" length="60" type="string" column="BRANCH_ID"/>
   <property name="busiType" length="20" type="string" column="BUSI_TYPE"/>
   <property name="acctTimeStart" type="timestamp" column="ACCT_TIME_START"/>
   <property name="acctTimeEnd" type="timestamp" column="ACCT_TIME_END"/>
   <property name="acctDate" type="java.util.Date" column="ACCT_DATE"/>
   <property name="acctType" length="10" type="string" column="ACCT_TYPE"/>
   <property name="acctUserId" length="40" type="string" column="ACCT_USER_ID"/>
   <property name="acctUserNo" length="40" type="string" column="ACCT_USER_NO"/>
   <property name="preFlowNo" length="60" type="string" column="PRE_FLOW_NO"/>
   <property name="endFlowNo" length="60" type="string" column="END_FLOW_NO"/>
   <property name="status" length="10" type="string" column="STATUS"/>
   <property name="errorMsg" length="200" type="string" column="ERROR_MSG"/>
   <property name="sourceId" length="40" type="string" column="SOURCE_ID"/>
   <property name="busiDate" type="java.util.Date" column="BUSI_DATE"/>
   <property name="billNo" length="40" type="string" column="BILL_NO"/>
   <property name="billAmt" type="big_decimal" column="BILL_AMT"/>
   <property name="valid" length="10" type="string" column="VALID"/>
  </class>
</hibernate-mapping>
