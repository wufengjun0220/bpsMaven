<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_TASK_TRANSFER" name="com.mingtech.application.sysmanage.domain.TaskTransfer" dynamic-insert="true" dynamic-update="true">
    <id type="string" column="id" length="40" name="id">
      <generator class="uuid"/>
    </id>
    <property name="transferorUid" length="40" type="string" column="TRANSFEROR_UID"/>
    <property name="transferorUname" length="80" type="string" column="TRANSFEROR_UNAME"/>
    <property name="transferorUcode" length="20" type="string" column="TRANSFEROR_UCODE"/>
    <property name="transferorUdeptId" length="40" type="string" column="TRANSFEROR_UDEPT_ID"/>
    <property name="transferorUdeptName" length="150" type="string" column="TRANSFEROR_UDEPT_NAME"/>
    <property name="transferedUid" length="40" type="string" column="TRANSFERED_UID"/>
    <property name="transferedUname" length="80" type="string" column="TRANSFERED_UNAME"/>
    <property name="transferedUcode" length="20" type="string" column="TRANSFERED_UCODE"/>
    <property name="transferedUdeptId" length="40" type="string" column="TRANSFERED_UDEPT_ID"/>
    <property name="transferedUdeptName" length="150" type="string" column="TRANSFERED_UDEPT_NAME"/>
    <property name="transferStatus" length="10" type="string" column="TRANSFER_STATUS"/>
    <property name="transferStartTime" type="timestamp" column="TRANSFER_START_TIME"/>
    <property name="transferStopTime" type="timestamp" column="TRANSFER_STOP_TIME"/>
    <property name="transferEndTime" type="timestamp" column="TRANSFER_END_TIME"/>
    <property name="createTime" type="timestamp" column="CREATE_TIME"/>
    <property name="transferRemark" length="10" type="string" column="TRANSFER_REMARK"/>
  </class>
</hibernate-mapping>
