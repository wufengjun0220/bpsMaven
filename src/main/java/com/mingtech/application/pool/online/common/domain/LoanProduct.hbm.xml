<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    核心放款接口产品代码字段映射表
-->
<hibernate-mapping>
    <class name="com.mingtech.application.pool.online.common.domain.LoanProduct" table="PED_LOAN_PRODUCT">
        <id name="id" type="string">
            <column name="ID" length="40" />
            <generator class="uuid.hex" />
        </id>
        <property name="loanProductNo" type="string">
            <column name="LOAN_PRODUCT_NO" length="10" />
        </property>
        <property name="loanProductName" type="string">
            <column name="LOAN_PRODUCT_NAME" length="150" />
        </property>
        <property name="deductionProductNo" type="string">
            <column name="DEDUCTION_PRODUCT_NO" length="10" />
        </property>
        <property name="deductionProductName" type="string">
            <column name="DEDUCTION_PRODUCT_NAME" length="150" />
        </property>
        <property name="subjectNo" type="string">
            <column name="SUBJECT_NO" length="10" />
        </property>
    </class>
</hibernate-mapping>
