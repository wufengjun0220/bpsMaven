<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="PED_BLACKLIST" name="com.mingtech.application.pool.common.domain.PedBlackList">
    <cache usage="read-write"/>
    <id type="string" column="id" length="40" name="id">
      <generator class="uuid"/>
    </id>
  
    <property name="createTime" type="timestamp" column="PED_CREATETIME" />
    <property name="type" type="string" column="PED_TYPE" />
    <property name="billMedia" type="string" column="PED_BILLMEDIA" />
    <property name="billType" type="string" column="PED_BILLTYPE" />
    <property name="dueDt" type="java.util.Date" column="PED_DUEDT" />
    <property name="keywords" type="string" column="PED_KEYWORDS" />
    <property name="content" type="string" column="PED_CONTENT" />
    <property name="dataFrom" type="string" column="PED_DATAFROM" />
    
    <property name="orgCode" type="string" column="PED_ORGCODE"/>
    <property name="customerName" type="string" column="PED_CUSTOMERNAME"/>
    <property name="netNo" type="string" column="PED_NETNO"/>
    <property name="tellerNo" type="string" column="PED_TELLERNO"/>
    
    <property name="province" type="string" column="PED_PROVINCE"/>
    <property name="city" type="string" column="PED_CITY"/>
    <property name="remark" type="string" column="REMARK"/>
  </class>
</hibernate-mapping>
