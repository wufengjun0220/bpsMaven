<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_TASK_TRANSFER_ROLE" name="com.mingtech.application.sysmanage.domain.TaskTransferRole" dynamic-insert="true" dynamic-update="true">
    <id type="string" column="id" length="40" name="id">
      <generator class="uuid"/>
    </id>
    <property name="transferId" length="40" type="string" column="TRANSFER_ID"/>
    <property name="transferedRoleId" length="80" type="string" column="TRANSFERED_ROLE_ID"/>
    <property name="transferedRoleName" length="20" type="string" column="TRANSFERED_ROLE_NAME"/>
  </class>
</hibernate-mapping>
